<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="ttk.Frame" id="frame1">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Label" id="w_name_label" named="True">
        <property name="text" translatable="yes">Full Name</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="w_name_entry" named="True">
        <property name="state">normal</property>
        <property name="textvariable">string:var_name_entry</property>
        <property name="validate">none</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="w_dinein_button" named="True">
        <property name="command" type="command" cbtype="simple">e_dinein</property>
        <property name="text" translatable="yes">Dine In</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="w_takeaway_button" named="True">
        <property name="command" type="command" cbtype="simple">e_takeaway</property>
        <property name="text" translatable="yes">Takeaway</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="w_delivery_button" named="True">
        <property name="command" type="command" cbtype="simple">e_delivery</property>
        <property name="text" translatable="yes">Delivery</property>
        <layout manager="grid">
          <property name="column">2</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="w_address_label" named="True">
        <property name="text" translatable="yes">Address</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="w_address_entry" named="True">
        <property name="textvariable">string:var_address_entry</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="columnspan">1</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="w_order_button" named="True">
        <property name="command" type="command" cbtype="simple">e_order</property>
        <property name="text" translatable="yes">Order</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">7</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Combobox" id="w_available_meals_combobox" named="True">
        <property name="textvariable">string:var_available_meals_combobox</property>
        <property name="values">"Chips" "Hamburger" "Cheeseburger" "Soda" "Tea" "Coffee"</property>
        <property name="width">10</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="w_available_meals_label" named="True">
        <property name="text" translatable="yes">Available Meals</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="w_selected_meals_label" named="True">
        <property name="text" translatable="yes">Selected Meals</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="tk.Text" id="w_selected_meals_textbox" named="True">
        <property name="height">5</property>
        <property name="insertunfocussed">none</property>
        <property name="pady">1</property>
        <property name="state">disabled</property>
        <property name="undo">false</property>
        <property name="width">10</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="w_add_meal_button" named="True">
        <property name="command" type="command" cbtype="simple">e_add_meal</property>
        <property name="text" translatable="yes">Add</property>
        <layout manager="grid">
          <property name="column">2</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="w_total_label" named="True">
        <property name="text" translatable="yes">Total: 0$</property>
        <property name="textvariable">string:var_total_label</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="row">6</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
